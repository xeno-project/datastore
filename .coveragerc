[run]
branch = True

[report]
omit =
    */_generated/*.py
    # Packages in the "google.cloud" package that we don't own.
    */google/cloud/gapic/*
    */google/cloud/grpc/*
    */google/cloud/proto/*
show_missing = True
exclude_lines =
    # Re-enable the standard pragma
    pragma: NO COVER
    # Ignore debug-only repr
    def __repr__
